{
  "name": "flowmate-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest --run",
    "test:coverage": "cross-env COVERAGE=true vitest --run --coverage",
    "preinstall": "node scripts/check-node.js",
    "clean:modules": "node -e \"require('fs').rmSync('node_modules',{recursive:true,force:true});\"",
    "clean:cache": "npm cache clean --force",
    "clean": "npm run clean:modules && npm run clean:cache",
    "reinstall": "npm run clean:modules && npm cache clean --force && npm install"
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16",
    "cross-env": "^7.0.3"
  }
}
